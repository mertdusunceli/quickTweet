{
  "manifest_version": 2,

  "name": "Twitter Social Toolkit",
  "description": "Allows you to tweet quotes with one click",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },

  "icons": {
    "64" : "icon.png",
    "32" : "icon.png",
    "16" : "icon.png",
    "128" : "icon.png"

  },

  "permissions" : [
    "contextMenus"
  ]


}
